{
    "env": {
        "node": true,
        "es2021": true
    },
    "plugins": [
        "pug"
    ],
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-prototype-builtins": 0,
        "no-var": 1
    }
}